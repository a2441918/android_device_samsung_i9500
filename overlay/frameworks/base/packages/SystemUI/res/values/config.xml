<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>

    <!-- Control whether status bar should distinguish HSPA data icon form UMTS
    data icon on devices -->
    <bool name="config_hspa_data_distinguishable">true</bool>

    <!-- Use camera interface for the flashlight. If set to false, you must specify
    config_flashDevice and config_torchBrightness too -->
    <bool translatable="false" name="config_useCameraInterface">false</bool>

    <bool name="config_status_bar_scrim_behind_use_src">false</bool>

</resources>
